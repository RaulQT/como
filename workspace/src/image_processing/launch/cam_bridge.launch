<!-- Launch file to start cam_bridge.py  -->

<launch>
  <!-- Start cam_bridge -->
  <node pkg="image_processing" type="cam_bridge.py" name="cam_bridge" output="screen" respawn="true">
    <rosparam command="load" file="$(find image_processing)/config/cam_bridge_params.yml"/>
  </node>
</launch>
